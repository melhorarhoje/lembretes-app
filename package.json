{
  "name": "compi-lembretes",
  "version": "1.0.0",
  "description": "COMPI - Painel de lembretes",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win portable"
  },
  "keywords": ["compi",
    "lembretes",
    "electron",
    "firebase"
  ],
  "author": "Seu Nome",
  "license": "MIT",
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4"
  },
  "build": {
    "appId": "com.compi.lembretes",
    "productName": "COMPI - Painel de lembretes",
    "directories": {
      "output": "dist"
    },
    "files": [
      "*.js",
      "*.html",
      "*.css",
      "*.mp3",
      "!node_modules",
      "!dist"
    ],
    "win": {
      "target": "portable"
    }
  }
}
